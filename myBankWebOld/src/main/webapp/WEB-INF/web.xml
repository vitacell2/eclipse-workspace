<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ListaClientesServlet</servlet-name>
		<display-name>ListaClientesServlet</display-name>
		<description></description>
		<servlet-class>com.curso.bank.web.ListaClientesServlet</servlet-class>
	</servlet>
  
	<servlet-mapping>
		<servlet-name>ListaClientesServlet</servlet-name>
		<url-pattern>/listaClientes</url-pattern>
	</servlet-mapping>
	
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
  
  	<!-- Esto importante que vaya lo ultimo, sino da error arriba en el <web-app> -->
	  <welcome-file-list>
		<welcome-file>/index.jsp</welcome-file>
	</welcome-file-list>
  
</web-app>