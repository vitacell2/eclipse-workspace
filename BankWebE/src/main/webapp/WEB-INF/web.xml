<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ListaClientesServlet</servlet-name>
  	<display-name>ListaClientesServlet</display-name>
  	<description></description>
  	<servlet-class>com.mybank.web.ListaClientesServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CuentasClienteServlet</servlet-name>
  	<display-name>CuentasClienteServlet</display-name>
  	<description></description>
  	<servlet-class>com.mybank.web.CuentasClienteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListaClientesServlet</servlet-name>
  	<url-pattern>/listaClientes</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CuentasClienteServlet</servlet-name>
  	<url-pattern>/cuentasCliente</url-pattern>
  </servlet-mapping>
  
  
  
  <welcome-file-list>
  	<welcome-file>/index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
